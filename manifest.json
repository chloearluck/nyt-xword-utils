  {
    "name": "New York Times Crossword Utilities",
    "version": "1.0",
    "description": "Toggle pencil tool with keyboard, hide navigation bar",
    "permissions": ["storage"],
    "content_scripts": [
        {
            "matches": [
                "https://www.nytimes.com/crosswords/*",
                "https://www.nytimes.com/puzzles/*"
            ],
            "js": ["xwordutils.js"]
        }
    ],
    "icons": {
        "16": "icons/xword-utils16.png",
        "48": "icons/xword-utils48.png",
        "128": "icons/xword-utils128.png"
    },
    "manifest_version": 2
  }
